{
  "hosting": {
    "site": "socketlab",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/socketlab/**/*.pdf",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/socketlab/implementations/(python|java|cpp|go|rust|dart)/**",
        "destination": "/socketlab/implementations/$1/index.html"
      },
      {
        "source": "/socketlab/**",
        "destination": "/socketlab/index.html"
      }
    ]
  }
}
